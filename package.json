{
  "name": "react-overlay-pack",
  "version": "3.0.7",
  "typings": "./index.d.ts",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "hsu-scripts src",
    "build-storybook": "build-storybook",
    "test": "NODE_ENV='test' jest --coverage",
    "test:watch": "npm run test -- --watch",
    "start": "start-storybook -p 6006",
    "eslint": "eslint ./",
    "flow": "flow",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,json,md,css,yaml,yml}' '*.{js,json,md,css,yaml,yml}'",
    "changelog": "github-changes -o evenchange4 -r react-overlay-pack -b master -f ./CHANGELOG.md --order-semver --use-commit-body"
  },
  "dependencies": {
    "dom-align": "^1.8.0",
    "prop-types": "^15.6.2",
    "raf-throttle": "^2.0.3",
    "ramda": "^0.26.1",
    "rc-tween-one": "^2.2.18",
    "react-portal": "^4.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.11",
    "@storybook/addon-info": "4.0.11",
    "@storybook/addon-links": "4.0.11",
    "@storybook/addon-options": "4.0.11",
    "@storybook/addon-storyshots": "4.0.11",
    "@storybook/react": "4.0.11",
    "codecov": "3.1.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.9.0",
    "flow-bin": "0.87.0",
    "flow-copy-source": "2.0.2",
    "github-changes": "1.1.2",
    "hsu-scripts": "0.1.0",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/*.example.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "resetMocks": true,
    "resetModules": true
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json,md,css,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "./node_modules/hsu-scripts/babel.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/hsu-scripts/eslint.js"
    ]
  },
  "description": "Reusable components to build Tooltip, Popover and Dialog.",
  "author": "Michael Hsu <evenchange4@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "portal",
    "overlay",
    "dialog",
    "popover",
    "tooltip"
  ],
  "bugs": {
    "url": "https://github.com/evenchange4/react-overlay-pack/issues",
    "email": "evenchange4@gmail.com"
  },
  "homepage": "https://github.com/evenchange4/react-overlay-pack#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/evenchange4/react-overlay-pack.git"
  }
}
