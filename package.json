{
  "name": "react-overlay-pack",
  "version": "3.0.2",
  "typings": "./index.d.ts",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "clean": "nwb clean-module && nwb clean-demo",
    "prebuild": "npm run clean",
    "build": "nwb build-react-component",
    "postbuild": "npm run build-flow",
    "build-flow": "flow-copy-source -i '**/*.test.js' -i '**/*.example.js' src es && flow-copy-source -i '**/*.test.js' -i '**/*.example.js' src lib",
    "build-storybook": "build-storybook",
    "test": "NODE_ENV='test' jest --coverage --runInBand",
    "test:watch": "npm run test -- --watch",
    "start": "start-storybook -p 6006",
    "eslint": "eslint ./",
    "flow": "flow",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,json,md}' '*.{json,md,js}'",
    "changelog": "github-changes -o evenchange4 -r react-overlay-pack -b master -f ./CHANGELOG.md --order-semver --use-commit-body"
  },
  "dependencies": {
    "dom-align": "^1.8.0",
    "prop-types": "^15.6.2",
    "raf-throttle": "^2.0.3",
    "ramda": "^0.25.0",
    "rc-tween-one": "^2.2.14",
    "react-portal": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-actions": "4.0.2",
    "@storybook/addon-info": "4.0.2",
    "@storybook/addon-links": "4.0.2",
    "@storybook/addon-options": "4.0.2",
    "@storybook/addon-storyshots": "4.0.2",
    "@storybook/react": "4.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-require-context-hook": "1.0.0",
    "codecov": "3.1.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.8.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-flowtype": "3.1.4",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.26.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.85.0",
    "flow-copy-source": "2.0.2",
    "github-changes": "1.1.2",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "lint-staged": "8.0.4",
    "nwb": "0.23.0",
    "prettier": "1.14.3",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-test-renderer": "16.6.0"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "require-context-hook",
          "@babel/plugin-proposal-class-properties"
        ],
        "presets": [
          [
            "@babel/preset-env"
          ],
          "@babel/preset-react",
          "@babel/preset-flow"
        ]
      }
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/*.example.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "resetMocks": true,
    "resetModules": true
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended",
      "plugin:jest/recommended",
      "prettier",
      "prettier/react",
      "prettier/flowtype"
    ],
    "plugins": [
      "prettier",
      "jest",
      "flowtype"
    ],
    "env": {
      "jest/globals": true
    },
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js"
          ]
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "react/forbid-prop-types": 0,
      "react/require-default-props": 0
    }
  },
  "description": "Reusable components to build Tooltip, Popover and Dialog.",
  "author": "Michael Hsu <evenchange4@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "portal",
    "overlay",
    "dialog",
    "popover",
    "tooltip"
  ],
  "bugs": {
    "url": "https://github.com/evenchange4/react-overlay-pack/issues",
    "email": "evenchange4@gmail.com"
  },
  "homepage": "https://github.com/evenchange4/react-overlay-pack#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/evenchange4/react-overlay-pack.git"
  }
}
